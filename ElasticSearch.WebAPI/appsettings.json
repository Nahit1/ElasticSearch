{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ElasticConnectionSettings": {
    "ElasticSearchHost": "http://localhost:9200",
    "ElasticLoginIndex": "login_log",
    "ElasticErrorIndex": "error_log",
    "ElasticAuditIndex": "audit_log"
  },
  "AllowedHosts": "*"
}
